
import axios from 'axios';

export default async function getGeoCode(address) {

    let country = address.country;
    let zip = address.zip;
    let api = process.env.WEATHER_KEY

    console.log(country, zip)
    let data = await axios.get(`http://api.openweathermap.org/geo/1.0/zip?zip=${zip},${country}&appid=${api}`)
        .then(res => res.data)
	.catch(err => console.log(err))
   
    return data
}
